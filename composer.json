{
    "name": "insanitymeetshh/file-sharing",
    "description": "File Sharing - InsanityMeetsHH",
    "version": "2.0.0-dev",
    "keywords": ["microframework", "rest", "router", "psr7", "doctrine", "twig", "localisation", "acl", "login", "csrf", "2fa"],
    "homepage": "http://fs.insanitymeetshh.net",
    "license": "MIT",
    "authors": [
        {
            "name": "InsanityMeetsHH",
            "email": "software@insanitymeetshh.net",
            "homepage": "http://www.insanitymeetshh.net"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5.0",
        "doctrine/orm": "^2.5",
        "geggleto/psr7-acl": "^1.3",
        "monolog/monolog": "^1.17",
        "slim/csrf": "^0.8.2",
        "slim/flash": "^0.4.0",
        "slim/slim": "^3.1",
        "slim/twig-view": "^2.3",
        "paragonie/random_compat": ">=1 <9.99",
        "phpgangsta/googleauthenticator": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "remove-cache": "rm -rf cache/*",
        "test": "phpunit",
        "setup": "App\\Composer\\Setup::run",
        "post-install-cmd": [
            "@setup"
        ],
        "post-update-cmd": [
            "@setup"
        ]
    }
}
